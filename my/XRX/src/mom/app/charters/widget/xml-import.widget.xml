                        <xrx:i18n>
                          <xrx:key>please-upload-a-xml-file-message</xrx:key>
                          <xrx:default>Please upload a XML file (*.xml).</xrx:default>
                        </xrx:i18n>
                      </xf:message>
                      <xf:dispatch if="ends-with(fileuri, '.xml')" name="ewizard1-2" targetid="mimport"/>
